<p><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
<%= form_for @user, url: '/get_meetup_info', method: :post do |f| %>
	<%= f.text_field :uid %> 
	<%=f.text_field :lat, id: 'lat'%>
	<%=f.text_field :lon, id: 'lon'%>
	<%= f.submit "Next", id: 'meetup', class: 'button' %>
<% end %>
</p>